<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/RelativeLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/nameView"
        android:layout_width="164dp"
        android:layout_height="57dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="232dp"
        android:layout_marginTop="-4dp"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:layout_marginStart="232dp"></TextView>

    <TextView
        android:id="@+id/lastNameView"
        android:layout_width="194dp"
        android:layout_height="50dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="0dp"
        android:textAppearance="?android:attr/textAppearanceLarge"></TextView>

    <TextView
        android:id="@+id/numberView"
        android:layout_width="370dp"
        android:layout_height="43dp"
        android:layout_below="@+id/nameView"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="48dp"
        android:textAppearance="?android:attr/textAppearanceLarge"></TextView>

    <TextView
        android:id="@+id/ReminderLabel"
        android:layout_width="150dp"
        android:layout_height="20dp"
        android:layout_below="@+id/numberView"
        android:layout_marginTop="5dp"
        android:text="ReminderFrequency: "></TextView>

    <TextView
        android:id="@+id/reminderView"
        android:layout_width="212dp"
        android:layout_height="20dp"
        android:layout_below="@+id/ReminderLabel"
        android:layout_alignParentRight="true"
        android:layout_marginTop="-17dp"
        android:layout_marginRight="40dp"></TextView>

</RelativeLayout>